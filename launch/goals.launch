<launch>
    <arg name="cop" default="deckard" />
    <arg name="robber" default="roy" />
    <arg name="policy" value="$(find pomdp_experiment)/policies/tagAvoidEmpty2.txt" />

    <node pkg="pomdp_experiment" type="goal_handler.py" name="cop_goal_handler"
        args="$(arg policy) $(arg cop) $(arg robber) -c" />
    <node pkg="pomdp_experiment" type="goal_handler.py" name="robber_goal_handler"
        args="$(arg policy) $(arg robber) $(arg cop) -r" />

</launch>
